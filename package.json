{
  "name": "reactoose",
  "version": "1.0.0",
  "description": "A project template for building front-end applications with React , Redux , tailwindcss and Typescript\n\n",
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "redux",
    "reactoose",
    "tailwindcss",
    "project template"
  ],
  "main": "index.js",
  "dependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux": "^3.6.0",
    "dotenv": "^8.0.0",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sha256": "^0.2.0",
    "typescript": "^3.2.2",
    "typescript-mix": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.10",
    "autoprefixer": "^9.5.1",
