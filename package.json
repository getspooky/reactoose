    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vorpal": "^1.12.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --mode development",
    "build": "npm run build:css && webpack --mode production",
    "test": "jest",
    "reactoose": "tsc reactoose-cli.ts && node reactoose-cli.js",
    "build:css": "postcss src/css/tailwind.css -o src/index.css",
    "watch:css": "postcss src/css/tailwind.css -o src/index.css -w"
